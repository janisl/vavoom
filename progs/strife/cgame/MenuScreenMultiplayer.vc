class MenuScreenMultiplayer:MenuScreen;

void InitWindow(void)
{
	if (!local_server)
	{
		Choices[0] = classidNone;
	}
	::InitWindow();
}

defaultproperties
{
	Choices[0] = MenuChoice_NewNetGame;
	Choices[1] = MenuChoice_JoinGame;
	Choices[2] = MenuChoice_PlayerSetup;
	ChoicesStartX = 97;
	ChoicesStartY = 40;
	SelectorType = MenuSelector_Spinner;
	Title = "MULTIPLAYER";
}

