class TeleportFog : Effect;

states
{
Spawn:
	TELE ABCDEFGH 6 Bright
	TELE G 6 Bright  A_Lights
	TELE FEDC 6 Bright
	Stop
}

defaultproperties
{
	DLightColour = RGB(202, 0, 0);
}
