	S_FOGPATCHL2('FOGL', 1, 7.0 / 35.0, S_FOGPATCHL3) { A_FogMove(); }
	S_FOGPATCHL3('FOGL', 2, 7.0 / 35.0, S_FOGPATCHL4) { A_FogMove(); }
	S_FOGPATCHL4('FOGL', 3, 7.0 / 35.0, S_FOGPATCHL5) { A_FogMove(); }
	S_FOGPATCHL5('FOGL', 4, 7.0 / 35.0, S_FOGPATCHL1) { A_FogMove(); }
	S_FOGPATCHL0('FOGM', 0, 4.0 / 35.0, S_FOGPATCHLA) { }
	S_FOGPATCHLA('FOGM', 1, 4.0 / 35.0, S_FOGPATCHLB) { }
	S_FOGPATCHLB('FOGM', 2, 4.0 / 35.0, S_FOGPATCHLC) { }
	S_FOGPATCHLC('FOGM', 3, 4.0 / 35.0, S_FOGPATCHLD) { }
	S_FOGPATCHLD('FOGM', 4, 4.0 / 35.0, S_FOGPATCHM0) { }
